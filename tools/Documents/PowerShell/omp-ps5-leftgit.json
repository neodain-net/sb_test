{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "transient_prompt": {},
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "folder_separator_icon": "\ue0b1",
            "max_depth": 3,
            "style": "folder"
          },
          "template": " \ue5ff  {{ .Path }} ",
          "foreground": "#FFFFFF",
          "powerline_symbol": "\ue0b0",
          "background": "#E9A41A",
          "type": "path",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_status": true,
            "fetch_status": true
          },
          "template": " \ue0a0 {{ .HEAD }}{{ if gt .Working.Modified 0 }} ~{{ .Working.Modified }}{{ end }}{{ if gt .Staging.Added 0 }} +{{ .Staging.Added }}{{ end }}{{ if gt .Working.Untracked 0 }} ?{{ .Working.Untracked }}{{ end }}{{ if gt .Ahead 0 }} \u21e1{{ .Ahead }}{{ end }}{{ if gt .Behind 0 }} \u21e3{{ .Behind }}{{ end }} ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#2ECC71",
          "type": "git",
          "style": "powerline"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
